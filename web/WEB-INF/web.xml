<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>com.app.Login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Home</servlet-name>
        <servlet-class>com.app.Home</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Logout</servlet-name>
        <servlet-class>com.app.Logout</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Appointments</servlet-name>
        <servlet-class>com.app.Appointments</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>StoreHome</servlet-name>
        <servlet-class>com.app.StoreHome</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>StoreAppointments</servlet-name>
        <servlet-class>com.app.StoreAppointments</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UserProfile</servlet-name>
        <servlet-class>com.app.UserProfile</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>StoreBookAppoint</servlet-name>
        <servlet-class>com.app.StoreBookAppoint</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/Login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Home</servlet-name>
        <url-pattern>/Home</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Logout</servlet-name>
        <url-pattern>/Logout</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Appointments</servlet-name>
        <url-pattern>/Appointments</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>StoreHome</servlet-name>
        <url-pattern>/StoreHome</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>StoreAppointments</servlet-name>
        <url-pattern>/StoreAppointments</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UserProfile</servlet-name>
        <url-pattern>/UserProfile</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>StoreBookAppoint</servlet-name>
        <url-pattern>/StoreBookAppoint</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
